<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_walk"
        android:name="com.example.doggywalk.ui.walk.WalkFragment"
        android:label="@string/navigation_walk"
        tools:layout="@layout/fragment_walk" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.doggywalk.ui.home.HomeFragment"
        android:label="@string/navigation_main"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.doggywalk.ui.profile.ProfileFragment"
        android:label="@string/navigation_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>